@model IEnumerable<BD_PR_01_Clinicas.Models.Rotaciones>

@{
    ViewBag.Title = "Index";
}

<h2>Rotaciones</h2>

<p>
    @Html.ActionLink("Nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Fecha de Inicio")
        </th>
        <th>
            @Html.DisplayName("Fecha de Finalización")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fechaInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaFin)
        </td>
        <td>
            @Html.ActionLink("Integrantes", "Integrantes", new { id=item.codigo }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>
