@model IEnumerable<BD_PR_01_Clinicas.Models.tbPresentacion>

@{
    ViewBag.Title = "Presentaciones";
}

<h2>Presentaciones</h2>

<p>
    @Html.ActionLink("Nueva Presentación", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Presentación")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.presentacion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.codPresentacion }) |
            @Html.ActionLink("Deshabilitar", "Delete", new { id=item.codPresentacion })
        </td>
    </tr>
}

</table>
